{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
      "arrow-parens": false,
      "no-bitwise": false,
      "no-var-requires": false,
      "object-literal-sort-keys": false,
      "ordered-imports": false,
      "quotemark": [
        true,
        "single",
        "avoid-escape"
      ],
      "trailing-comma": [
        true,
        {
          "multiline": "never",
          "singleline": "never"
        }
      ]
    },
    "rulesDirectory": []
}
